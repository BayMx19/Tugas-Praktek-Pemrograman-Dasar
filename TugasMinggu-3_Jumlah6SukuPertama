#include <iostream>
#include <math.h>

using namespace std;

int main()

{	
	int Jumlah=0;
	float i, Pangkat;
	cout<< endl;
	for(int i = 1; i<=6; i++)
	{
		Pangkat = pow(i,3);
		cout<<"Suku ke-"<< i; cout<<"  Pangkat 3	=>> "<< Pangkat<< endl;
		cout<<"\n \n"<<endl;
		
		Jumlah+=Pangkat, Pangkat++;


	} 
	cout<<"==============================="<<endl;
	cout<<" JUMLAH HASIL PANGKAT =>> "<< Jumlah<< endl;
	cout<<"==============================="<<endl;
	cout<<"\n \n"<<endl;
	
	cout<<"Terimakasih Telah Menggunakan Program Ini :)"<<endl;
	cout<<"SEMOGA SEHAT SELALU"<<endl;
	cout<< endl;
	
	return 0;
}
